<?xml version="1.0"  encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <EmscriptenVersion Condition="'$(EmscriptenVersion)' == '' And '$(TargetFramework)' == 'net6.0'">2.0.23</EmscriptenVersion>
    <EmscriptenVersion Condition="'$(EmscriptenVersion)' == '' And '$(TargetFramework)' == 'net7.0'">3.1.12</EmscriptenVersion>
    <EmscriptenVersion Condition="'$(EmscriptenVersion)' == '' And '$(TargetFramework)' == 'net8.0'">3.1.34</EmscriptenVersion>
    <EmscriptenVersion Condition="'$(EmscriptenVersion)' == '' And ('$(TargetFramework)' == 'net9.0' Or '$(TargetFramework)' == 'net10.0')">3.1.56</EmscriptenVersion>
  </PropertyGroup>
  <ItemGroup Condition="'$(TargetOS)' == 'Browser'">
    <NativeFileReference Include="$(MSBuildThisFileDirectory)..\..\runtimes\browser-wasm\$(EmscriptenVersion)\openjtalk-lang.a" />
  </ItemGroup>
</Project>